{
  "name": "niko-bot",
  "version": "0.0.1",
  "description": "A \"Fast Than Light\" Bot to buy online shoes",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "npm run lint",
    "build": "npm-run-all clean lint && tsc",
    "prestart": "npm run build",
    "dev:start": "npm run build && cross-env NODE_ENV=development node .",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "cross-env NODE_ENV=production node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielMochi/niko-bot.git"
  },
  "keywords": [
    "Shoes",
    "Nike",
    "Adidas",
    "Online",
    "Buy"
  ],
  "author": "Gabriel Mochi <gmochi56@icloud.com>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "bugs": {
    "url": "https://github.com/GabrielMochi/niko-bot/issues"
  },
  "homepage": "https://github.com/GabrielMochi/niko-bot#readme",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.12",
    "@types/puppeteer-core": "^2.0.0",
    "@types/qs": "^6.9.3",
    "@types/socket.io": "^2.1.8",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "metadata": "^0.1.0",
    "puppeteer-core": "^5.0.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  }
}
